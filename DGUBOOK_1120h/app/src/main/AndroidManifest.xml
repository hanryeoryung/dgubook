<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.dgubook" >
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".main.MainLoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.MainHomeAtivity"
            android:label="@string/app_name" />
        <activity
            android:name=".action_book.ReserveAtivity"
            android:label="@string/app_name" />
        <activity
            android:name=".action_book.SearchAtivity"
            android:label="@string/app_name" />
        <activity
            android:name=".action_book.UploadAtivity"
            android:label="@string/app_name" />
        <activity
            android:name=".join.JoinActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".join.JoinConfirmActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".messenger.MessengerAtivity"
            android:label="@string/app_name" />
        <activity
            android:name=".setting.InfoAtivity"
            android:label="@string/app_name" />

        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name" />

    </application>

</manifest>
